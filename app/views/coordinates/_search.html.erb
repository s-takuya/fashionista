<%= form_tag coordinates_path, method: :get do %>
  <%= select_tag :owner_id, options_from_collection_for_select(Owner.all, :id, :name, params[:owner_id]), { include_blank: 'people' } %>
  <%= button_tag type: 'submit', class: 'btn btn-default' do %>
    <i class='fa fa-search'></i>
  <% end %>
<% end %>
